{
  "extends": [
      "airbnb-base",
      "plugin:import/recommended"
  ],
  "rules": {
    "no-console": "off",
    "import/extensions": "off",
    "import/prefer-default-export": "off",
    "quotes": ["error", "double"],
    "no-underscore-dangle": [
      "error",
      {
        "allow": [
         "_id"
        ]
      }
    ]
  }
}
